{
  "name": "vue-google-charts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "server": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toyjhlee/vue-google-charts.git"
  },
  "keywords": [],
  "author": {
    "name": "toyjhlee",
    "email": "toyjhlee@gmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/toyjhlee/vue-google-charts/issues"
  },
  "homepage": "https://github.com/toyjhlee/vue-google-charts#readme",
  "dependencies": {
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "husky": "^3.0.5",
    "jest": "^21.2.1",
    "jest-serializer-vue": "^0.2.0",
    "vue-jest": "^2.6.0",
    "vue-loader": "^13.0.5",
    "vue-server-renderer": "^2.4.4",
    "vue-template-compiler": "^2.4.4",
    "semantic-release": "^17.2.3",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "text",
      "json",
      "html",
      "cobertura"
    ],
    "setupFiles": [
      "./jest.test.setup.js"
    ],
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "modulePaths": [
      "<rootDir>/node_modules"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
